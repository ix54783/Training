def turn_right():
    turn_left()
    turn_left()
    turn_left()

while front_is_clear():
    move()
#essential step to breakout the loop
turn_left()
         
    
def jump():
    
    if front_is_clear() and right_is_clear():
        turn_right()
        move()
    elif front_is_clear() and wall_on_right():
        move()
